{
  "manifest_version": 2,
  "name": "mini",
  "description": "Attendence for",
  "key": "",
  "version": "1.0.1",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "identity",
    "scripting",
    "storage",
    "videoCapture"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "signin.html"
  },
  "background": {
    "scripts": [
      "background.js",
      "popup.js"
    ]
  },
  "sandbox": {
    "pages": [
      "index.html"
    ]
  },
  "content_security_policy": "script-src 'self' default-src https://apis.google.com https://www.gstatic.com https://www.googleapis.com https://securetoken.googleapis.com ; object-src 'self' "
}