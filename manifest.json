{
  "manifest_version": 2,
  "name": "mini",
  "description": "Quick launch Traversy Media",
  "version": "1.0.0",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "popup.js"
    ]
  }
}